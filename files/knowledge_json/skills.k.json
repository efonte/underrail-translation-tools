      "LibraryId": 2,
      "Values": [
        29,
        {
          "ObjectId": 3,
          "Value": "generalinfo",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 4,
          "Value": "As your character levels up, they will receive a number of skill points that you can distribute in various areas. You may not invest more than 5 skill points (three times that at level one) per level in any given skill.\r\n\r\nEach skill is affected by one of your character's base ability scores, but the amount of points you may invest in any skill is not restricted by them.\r\n\r\nSome skills may benefit from others through skill synergy.\r\n\r\nThe number in brackets represents the skill level with all factors counted in.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 5,
          "Value": "guns",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 6,
          "Value": "Increases your precision and damage with any type of pistol, SMG or rifle. This includes both firearms and energy weapons.\r\n\r\nAlso, the higher your skill level the more advanced weapons you can wield effectively.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 7,
          "Value": "guns_xpbl",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 8,
          "Value": "Increases your precision and damage with any type of pistol, SMG, shotgun, or rifle. This includes both firearms and energy weapons.\r\n\r\nAlso, the higher your skill level the more advanced weapons you can wield effectively.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 9,
          "Value": "guns_xphw",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 10,
          "Value": "Increases your precision and damage with any type of pistol, SMG, rifle, or grenade launcher. This includes both firearms and energy weapons.\r\n\r\nAlso, the higher your skill level the more advanced weapons you can wield effectively.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 11,
          "Value": "guns_full",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 12,
          "Value": "Increases your precision and damage with any type of pistol, SMG, shotgun, rifle, or grenade launcher. This includes both firearms and energy weapons.\r\n\r\nAlso, the higher your skill level the more advanced weapons you can wield effectively.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 13,
          "Value": "heavy guns",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 14,
          "Value": "Increases your precision and damage with machine guns and handheld miniguns.\r\n\r\nAlso, the higher your skill level the more advanced weapons you can wield effectively.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 15,
          "Value": "crossbows",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 16,
          "Value": "Increases your precision and damage with crossbows.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 17,
          "Value": "grenades-old",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 18,
          "Value": "Allows you to throw grenades more quickly, more often and across greater distance.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 19,
          "Value": "throwing",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 20,
          "Value": "Increases your precision with all thrown weapons and utilities, including grenades.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 21,
          "Value": "melee",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 22,
          "Value": "Increases mechanical damage with melee weapons and unarmed attacks and helps you overcome your target's dodge rating.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 23,
          "Value": "dodge",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 24,
          "Value": "Increases your chance to avoid melee attacks.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 25,
          "Value": "evasion",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 26,
          "Value": "Increases your chance to avoid a ranged attack. Reduces damage you receive from area of effect attacks.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 27,
          "Value": "stealth",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 28,
          "Value": "Decreases the chance that you'll be spotted while in stealth mode. Also affects numerous aspects of stealth combat.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 29,
          "Value": "hacking",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 30,
          "Value": "Allows you to hack all kinds of computer devices, such as electronic locks, cameras, sentry turrets, etc.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 31,
          "Value": "lockpicking",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 32,
          "Value": "Allows you to pick mechanical locks and dismantle other mechanical barriers such as ventilation shafts.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 33,
          "Value": "pickpocketing",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 34,
          "Value": "Allows you to steal items from other characters.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 35,
          "Value": "mechanics",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 36,
          "Value": "Represents your knowledge of mechanics. This skill is primarily used in crafting various items, such as weapons, armors and traps.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 37,
          "Value": "electronics",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 38,
          "Value": "Represents your knowledge of electronics. This skill is primarily used in crafting various items, such as various electronic utilities and energy weapons.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 39,
          "Value": "chemistry",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 40,
          "Value": "Represents your knowledge of chemistry. This skill is primarily used in crafting various items, such as explosives.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 41,
          "Value": "biology",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 42,
          "Value": "Represents your knowledge of biology. This skill is primarily used in crafting various items, such as healing and psi restoring concoctions. It also enables you to extract useful substances from plants and animals.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 43,
          "Value": "tailoring",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 44,
          "Value": "Represents your skill in tailoring leather and cloth materials into pieces of armor.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 45,
          "Value": "thought control",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 46,
          "Value": "Ability to influence the minds of other living creatures through mental suggestion and domination. Also the ability to focus and enhance your own mental processes and to take greater control of your body.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 47,
          "Value": "psychokinesis",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 48,
          "Value": "Ability of the mind to exert mechanical force, as well as to emit electric currents and project electromagnetic fields.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 49,
          "Value": "metathermics",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 50,
          "Value": "Ability of mind to initiate and control rapid temperature changes and harness exothermic and endothermic energies.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 51,
          "Value": "temporal manipulation",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 52,
          "Value": "Ability of mind to manipulate time or time perception on very local level, including partial time suspension, reversal and recurrence.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 53,
          "Value": "persuasion",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 54,
          "Value": "Your ability to convince the other party to see things your way through non-violent means.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 55,
          "Value": "intimidation",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 56,
          "Value": "Your ability to make someone act out of fear.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 57,
          "Value": "mercantile",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 58,
          "Value": "Your ability to work out favorable prices in a business transaction.",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 59,
          "Value": "traps",
          "RecordTypeEnum": "BinaryObjectString"
        },
        {
          "ObjectId": 60,
          "Value": "Allows you to plant and disarm increasingly more powerful traps. The higher your skill is the harder your traps are to spot and disarm.\r\n\r\nAlso increases your chance of spotting traps.",
          "RecordTypeEnum": "BinaryObjectString"
        }
      ],
